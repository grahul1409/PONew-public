{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.18.3",
      "toolsId": "8eb146ba-7ca5-48a9-962b-a0894e6da9dc"
    },
    "dataSources": {
      "mainService": {
        "uri": "/poapproval/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.138.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {          
          "sap.fe.templates.ObjectPage.ObjectPageController#project1::PurchaseOrdersObjectPage": {
            "controllerName": "project1.ext.controller.Reject"
          },
          "sap.fe.templates.ListReport.ListReportController#project1::PurchaseOrdersList": {
            "controllerName": "project1.ext.controller.UserInfoController"
          }
          
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/custom.css"
        }
      ]
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "PurchaseOrdersList",
          "target": "PurchaseOrdersList"
        },
        {
          "pattern": "PurchaseOrders({key}):?query:",
          "name": "PurchaseOrdersObjectPage",
          "target": "PurchaseOrdersObjectPage"
        },
        {
          "pattern": "PurchaseOrders({key})/items({key2}):?query:",
          "name": "POItemsObjectPage",
          "target": "POItemsObjectPage"
        }
      ],
      "targets": {
        "PurchaseOrdersList": {
          "type": "Component",
          "id": "PurchaseOrdersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/PurchaseOrders",
              "variantManagement": "Page",
              "initialLoad": "Enabled",
              "adaptFiltersEnabled": false,
              "filterBar": {
                "hiddenFilters": ["user"],
                "adaptFiltersEnabled": false,
                "hideAdaptFiltersButton": true
              },
              "navigation": {
                "PurchaseOrders": {
                  "detail": {
                    "route": "PurchaseOrdersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "FilterMyPOs": {
                      "press": ".extension.project1.ext.controller.UserInfoController.onFilterMyPOs",
                      "visible": true,
                      "enabled": true,
                      "text": "ðŸ“‹ My POs"
                    }
                  }
                }
              }
            }
          }
        },
        "PurchaseOrdersObjectPage": {
          "type": "Component",
          "id": "PurchaseOrdersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/PurchaseOrders",
              "enhanceI18n": "i18n/customI18N.properties",
              "navigation": {
                "items": {
                  "detail": {
                    "route": "POItemsObjectPage"
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "Delete": {
                      "visible": false
                    },
                    "DataFieldForAction::Delete": {
                      "visible": false
                    },
                    "UserInfo": {
                      "press": ".extension.project1.ext.controller.Reject.onShowUserInfo",
                      "visible": true,
                      "enabled": true,
                      "text": "Reject"
                    }
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.Identification": {
                  "actions": {
                    "Delete": {
                      "visible": false
                    },
                    "DataFieldForAction::Delete": {
                      "visible": false
                    }
                  }
                }
              }
            }
          }
        },
        "POItemsObjectPage": {
          "type": "Component",
          "id": "POItemsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/PurchaseOrders/items"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
